package chap7;
final class Math{
	public static int abs(int data)
	{
		if(data<0) return data*-1;
		else return data;
	}
}
class MyMath{
	void power() {
		System.out.println("제곱을 구합니다.");
	}
}
public class MathTest {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		MyMath m = new MyMath();
		m.power();
		System.out.println(Math.abs(-10));
	}

}
